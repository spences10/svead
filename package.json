{
	"name": "svead",
	"description": "Svead, a component that allows you to set head meta information, canonical, title, Twitter and Facebook Open Graph tags.",
	"version": "0.0.4",
	"author": {
		"name": "Scott Spence",
		"email": "yo@scottspence.dev",
		"url": "https://scottspence.com"
	},
	"keywords": [
		"svelte",
		"sveltekit",
		"head",
		"meta-tags"
	],
	"scripts": {
		"dev": "vite dev",
		"build": "pnpm un svead && pnpm i -D svead && vite build",
		"package": "svelte-package",
		"preview": "vite preview",
		"test": "playwright test",
		"test:ci": "vitest run",
		"test:unit": "vitest",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "pretty-quick",
		"package:local": "svelte-kit package && pnpm i -D ./package",
		"contributors:add": "all-contributors add",
		"contributors:generate": "all-contributors generate"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.1",
		"@sveltejs/package": "1.0.2",
		"@tailwindcss/typography": "^0.5.8",
		"@typescript-eslint/eslint-plugin": "^5.46.0",
		"@typescript-eslint/parser": "^5.46.0",
		"all-contributors-cli": "^6.24.0",
		"autoprefixer": "^10.4.13",
		"daisyui": "^2.43.0",
		"eslint": "^8.29.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"fathom-client": "^3.5.0",
		"husky": "^8.0.2",
		"mdsvex": "^0.10.6",
		"postcss": "^8.4.19",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.1",
		"prettier-plugin-svelte": "^2.9.0",
		"pretty-quick": "^3.1.3",
		"svead": "^0.0.4",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.0",
		"svelte-preprocess": "^5.0.0",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.4.1",
		"typescript": "^4.9.4",
		"vite": "^4.0.0",
		"vitest": "^0.28.0"
	},
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/spences10/svead.git"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/spences10/svead/issues"
	},
	"homepage": "https://github.com/spences10/svead#readme"
}
